    "version": "0.2.0",
    "configurations": [
        {
            "name": "dev-debug (android)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--flavor",
                "dev",
                "--dart-define",
                "cameraPreviewAspectRatio=240/320",
            ],
            "program": "${workspaceFolder}/lib/main_dev.dart",
        },
        {
            "name": "dev-profile (android)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--flavor",
                "dev",
                "--dart-define",
                "cameraPreviewAspectRatio=240/320",
            ],
            "program": "${workspaceFolder}/lib/main_dev.dart",
        },
        {
            "name": "prod-debug (android)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--flavor",
                "prod",
                "--dart-define",
                "cameraPreviewAspectRatio=240/320",
            ],
            "program": "${workspaceFolder}/lib/main_release.dart",
        },
        {
            "name": "prod-profile (android)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--flavor",
                "prod",
                "--dart-define",
                "cameraPreviewAspectRatio=240/320",
            ],
            "program": "${workspaceFolder}/lib/main_release.dart",
        },
        {
            "name": "dev-debug (ios)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--flavor",
                "dev",
