        },
        {
            "name": "dev-profile (ios)",
            "request": "launch",
            "flutterMode": "profile",
            "type": "dart",
            "args": [
                "--flavor",
                "dev",
                "--dart-define",
                "cameraPreviewAspectRatio=3/4",
            ],
            "program": "${workspaceFolder}/lib/main_dev.dart",
        },
    ],
