    "version": "0.2.0",
    "configurations": [
        {
            "name": "dev-debug",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--flavor",
                "dev",
            ],
            "program": "${workspaceFolder}/lib/main_dev.dart",
        },
        {
            "name": "dev-profile",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--flavor",
                "dev",
            ],
            "program": "${workspaceFolder}/lib/main_dev.dart",
        },
        {
            "name": "prod-debug",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--flavor",
                "prod",
            ],
            "program": "${workspaceFolder}/lib/main_prod.dart",
        },
        {
            "name": "prod-profile",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--flavor",
                "prod",
            ],
            "program": "${workspaceFolder}/lib/main_prod.dart",
        },
        {
            "name": "dev-simulator",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--flavor",
                "dev",
                "--dart-define",
                "cameraStubImage=assets/camera_stub_image.jpg"
            ],
            "program": "${workspaceFolder}/lib/main_dev.dart",
        },
        {
            "name": "dev-simulator",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--flavor",
                "dev",
                "--dart-define",
                "cameraPreviewAspectRatio=240/320",
                "--dart-define",
                "cameraStubImage=assets/camera_stub_image.jpg"
            ],
            "program": "${workspaceFolder}/lib/main_dev.dart",
        },
    ],
}